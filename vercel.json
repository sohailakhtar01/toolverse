{
  "redirects": [
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "toolverse-brown.vercel.app"
        }
      ],
      "destination": "https://thetoolsverse.com",
      "permanent": true
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "toolverse-brown.vercel.app"
        }
      ],
      "destination": "https://thetoolsverse.com/:path*",
      "permanent": true
    }
  ]
}
